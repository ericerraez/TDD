fun calcularAreaRectangulo(base: Double, altura: Double): Double {
    return base * altura
}

// Prueba
fun testCalcularAreaRectangulo() {
    val result = calcularAreaRectangulo(5.0, 8.0)
    assert(result == 40.0) { "El Ã¡rea calculada no coincide con el valor esperado" }
